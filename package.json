{
  "name": "api-autenticacao",
  "version": "1.0.0",
  "private": true,
  "description": "Api rest para autenticação via LDAP",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec 'ts-node src/entry.ts' -e ts",
    "test": "jest --no-cache",
    "build": "tsc",
    "prod": "npm run build && node dist/entry.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "coverage": "jest --coverage --coverageDirectory='coverage' --collectCOverageFrom='src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "faker": "^4.1.0",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "date-fns": "^2.15.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
